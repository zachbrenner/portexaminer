<h3>we searched for <%= @keywords.join(", ") %></h1>
<table class="table">
	<thead>
		<tr>
			<th scope="col">#</th>
			<th scope="col">Title</th>
			<th scope="col">Blurb</th>
		</tr>
	</thead>
	<tbody>
		<% @chart.each do |link, blurb| %>
			<tr>
				<th scope="row"><%="#{link[1]} #{link[0]}" %> </th>
				<td><a href= <%= link[2] %> target="_blank"><%=link[2]%></a></td>
				<td><%= blurb %></td>
			</tr>
		

		<% end %>

	</tbody>
</table>